<view class="personal_info">
	<view class="photo_wrap">
		<open-data type="userAvatarUrl"></open-data>
	</view>
	<view class="nickname">
		<open-data type="userNickName"></open-data>
	</view>
</view>
<view class="wode_item_wrap">
	<view class="wode_item" wx:for="{{mode}}" wx:key="key">
		{{item}}
